  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "bluebird": "^3.4.7",
    "co-mocha": "^1.1.3",
    "code": "^4.0.0",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "mockery": "^2.0.0"
  },
  "dependencies": {
    "botkit": "^0.5.2",
    "debug": "^2.3.3",
    "sync-request": "^3.0.1"
